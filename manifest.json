{
    "manifest_version": 2,
    "name": "Cookie Canister",
    "description": "A simple extension for reviewing, managing, and exporting your cookies.",
    "version": "0.0.0.1",
     "browser_action": {},
    "permissions": [
        "storage",
        "<all_urls>",
        "cookies",
        "tabs",
         "webRequest",
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ],
    "web_accessible_resources": [
        
    ],
    "background":
    {
        "scripts": ["cookiescrape.js"],
        "persistent": true
    }
}